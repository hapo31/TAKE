{
  "name": "take",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "electron ./dist",
    "format": "prettier --write src/**/*",
    "watch": "webpack --watch --config ./webpack.config.js --mode development",
    "build:win": "cross-env NODE_ENV=production webpack --config ./webpack.config.js --mode production && copy package.json dist\\ && electron-packager ./dist TAKE --platform=win32 --arch=x64 --icon=./images/icon.ico --electron-version=5.0.2 --overwrite && copy readme.md TAKE-win32-x64\\readme.txt"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.9",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/styled-components": "^5.1.1",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "electron": "^9.1.0",
    "electron-packager": "^15.0.0",
    "prettier": "^2.0.5",
    "ts-loader": "^8.0.1",
    "typescript": "^3.5.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "fluent-ffmpeg": "^2.1.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
